
@{
    ViewBag.Title = "GetTournament";
}

<h2>Get Tournament From Challong</h2>
<p>@ViewBag.response</p>



<form method="get">
    <div class="form-group">
        <label for="search">Challong Tournament URL</label>
        <input class="form-control" id="search" name="search" type="text" placeholder="Enter URL...">
    </div>
    <div class="form-group">
        <label for="api_key">Challong Account Api Key</label>
        <a class="font-weight-light" href="https://challonge.com/settings/developer">Can be found here</a>
        <input class="form-control" id="api_key" name="api_key" type="text" placeholder="Enter Api Key...">
    </div>
    <div class="form-check">
        <input class="button" type="submit" value="Search" />
    </div>
</form>



<b>@ViewBag.Success</b>